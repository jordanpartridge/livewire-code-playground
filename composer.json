{
    "name": "jordanpartridge/livewire-code-playground",
    "description": "A Livewire-powered code playground component for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Partridge",
            "email": "jordan.partridge@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "JordanPartridge\\LivewireCodePlayground\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JordanPartridge\\LivewireCodePlayground\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JordanPartridge\\LivewireCodePlayground\\LivewireCodePlaygroundServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
